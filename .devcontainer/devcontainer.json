{
    "name": "gip-python310-uv",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "dev",
    "workspaceFolder": "/workspaces",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    },
    "remoteUser": "vscode",
    // "remoteEnv": {
    //   "PATH": "/opt/venv/bin:${PATH}"
    // },
    "postCreateCommand": "git config --global --add safe.directory /workspaces && /opt/venv/bin/uv pip list && python main.py"
  }
  